{
    "manifest_version" : 3,
    "name" : "Pomo-Tempo",
    "version" : "1.0",
    "description" : "Une application pomodoro",
    "permissions" : [
        "activeTab"
    ],
    "action" : {
        "default_icon" : "icon.png",
        "default_popup" : "index.html",
        "default_popup_options": {
            "width" : 300, 
            "height" : 200
        }
    },
    "icons" : {
        "48" : "icon.png"
    }
}